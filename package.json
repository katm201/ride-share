{
  "name": "ride-share",
  "version": "1.0.0",
  "description": "driver inventory micro-service for a ride-sharing clone",
  "scripts": {
    "db:add-blocks": "psql -U postgres -d inventory -f $(pwd)/service/data/2010_sf_census_tracts/SHAPEFILE.sql",
    "db:add-blocks-auto": "shp2pgsql -I -s 4326 $(pwd)/service/data/2010_sf_census_tracts/geo_export_c0e33c61-c33d-4f06-b4f6-0d12c4f9156f.shp census_blocks | psql -U postgres -d inventory",
    "db:add-blocks-docker": "node ./service/src/database/setup/census-blocks.js",
    "db:build": "node ./service/src/database/setup/tables.js",
    "db:seed": "node ./service/src/database/setup/seed.js",
    "db:setup": "npm run db:add-blocks && npm run db:build && npm run db:seed",
    "db:setup-docker": "npm run db:add-blocks-docker && npm run db:build && npm run db:seed",
    "req:ping": "node ./load-test/ping.js",
    "req:new-ride": "node ./load-test/new-ride.js",
    "req:new-ride-sustained": "node ./load-test/sustained/new-ride.js",
    "req:new-driver": "node ./load-test/new-driver.js",
    "req:complete-driver": "node ./load-test/complete-driver.js",
    "req:update-driver": "node ./load-test/update-driver.js",
    "start": "NODE_ENV=production node ./service/src/server/index.js",
    "start:dev": "nodemon ./service/src/server/index.js",
    "test:dev": "./node_modules/.bin/mocha -t 7000 -R nyan",
    "test": "./node_modules/.bin/mocha -t 7000 -R list --exit",
    "coverage:db": "node ./node_modules/.bin/istanbul cover  test/dbSpec.js",
    "coverage:server": "node ./node_modules/.bin/istanbul cover test/serverSpec.js",
    "artillery:ping-local": "node ./node_modules/.bin/artillery run hello-local.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katm201/ride-share.git"
  },
  "author": "Katherine Manning",
  "license": "ISC",
  "homepage": "https://github.com/katm201/ride-share#readme",
  "devDependencies": {
    "artillery": "^1.6.0-12",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.5",
    "sinon": "^4.1.3"
  },
  "dependencies": {
    "aws-sdk": "^2.171.0",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bookshelf": "^0.12.0",
    "bookshelf-postgis": "^0.2.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "knex": "^0.14.2",
    "knex-postgis": "^0.5.0",
    "kue": "^0.11.6",
    "newrelic": "^2.4.2",
    "node": "^9.3.0",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "prompt": "^1.0.0"
  }
}
